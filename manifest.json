{
  "manifest_version": 2,
  "name": "Facebook Dimmer",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "https://www.facebook.com/*", "http://www.facebook.com/*"
    ],
    "js": ["timeme.min.js", "dimmer.js"]
  }],
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
